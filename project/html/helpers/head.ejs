    <script src="https://www.gstatic.com/firebasejs/7.2.1/firebase-database.js"></script>

<script>
  // Your web app's Firebase configuration
  var firebaseConfig = {
    apiKey: "<%= process.env.FIREBASE_API_KEY %>",
    authDomain: "web-app-e40f0.firebaseapp.com",
    databaseURL: "https://web-app-e40f0.firebaseio.com",
    projectId: "web-app-e40f0",
    storageBucket: "web-app-e40f0.appspot.com",
    messagingSenderId: "529385963783",
    appId: "1:529385963783:web:3ef867333cde9a2ddb45a2"
